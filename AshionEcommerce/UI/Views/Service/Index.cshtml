@model List<ENTITY.Concrete.Service>

<section class="services spad">
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-4 col-sm-6">
                    <div class="services__item">
                        <i class="@item.Icon"></i>
                        <h6>@item.Title</h6>
                        <p>@item.SubTitle</p>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
